{
  "presets": [
    "@babel/env",
    "@babel/typescript",
    [
      "@babel/react",
      {
        "runtime": "automatic",
        "importSource": "@emotion/react"
      }
    ]
  ],
  "plugins": [
    "macros",
    "@babel/transform-runtime",
    [
      "module-resolver", {
        "root": ["./src"],
        "alias": {
          "@b-core": "./core",
          "@b-components": "./components"
        }
      }
    ]
  ]
}
